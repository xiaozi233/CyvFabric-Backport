{
	"schemaVersion": 1,
	"id": "cyvfabric",
	"version": "${version}",
	"name": "CyvFabric",
	"description": "CyvClient parkour features ported to modern FabricMC.",
	"authors": [
		"Morpheye"
	],
	"contact": {
		"homepage": "https://modrinth.com/project/cyvfabric"
	},
	"icon": "assets/cyvfabric/icon.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			"net.cyvfabric.CyvFabric"
		],
		"client": [
			"net.cyvfabric.CyvFabricClient"
		],
		"fabric-datagen": [
			"net.cyvfabric.CyvFabricDataGenerator"
		]
	},
	"accessWidener" : "cyvfabric.accesswidener",
	"mixins": [
		"cyvfabric.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.5",
		"minecraft": "~1.18.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}